             font="SansSerifBold"
             height="15"
             layout="topleft"
             left="9"
             name="title_fl_descr_text"
             text_color="white"
             top_pad="10"
             width="290">
                First Life description:
            </text>
            <text
             type="string"
             follows="left|top|right"
             font="SansSerif"
             height="45"
             layout="topleft"
             left="9"
             name="fl_description_edit"
             text_color="0.1 0.1 0.1 1"
