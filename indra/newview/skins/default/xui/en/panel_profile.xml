            <text
             type="string"
             follows="left|top"
             font="SansSerifBold"
             height="15"
             layout="topleft"
             name="title_partner_text"
             text_color="white"
             top_pad="10"
             width="100">
                Partner:
            </text>
            <panel
             follows="left|top|right"
             height="15"
             layout="topleft"
             left="9"
             name="partner_data_panel"
             width="335">
                <text
                 type="string"
                 follows="left|top|right"
                 font="SansSerif"
                 height="15"
                 layout="topleft"
                 left="0"
                 name="partner_text"
                 text_color="0.1 0.1 0.1 1"
                 top="0"
                 width="290"
                 word_wrap="true">
                    [FIRST] [LAST]
